<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_5_0.xsd"
         version="5.0">

  <servlet>
    <servlet-name>UserGetServlet</servlet-name>
    <servlet-class>com.example.UserGetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserGetServlet</servlet-name>
    <url-pattern>/getAll</url-pattern>
  </servlet-mapping>

  <display-name>PassportProject</display-name>
  <welcome-file-list>
    <welcome-file>response.jsp</welcome-file>
  </welcome-file-list>

</web-app>
